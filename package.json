{
  "name": "jamsticnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev -p 3001",
    "dev-ts": "concurrently -n NEXT,TS -c magenta,cyan \"next dev -p 3001\" \"pnpm run tsw\"",
    "tsw": "tsc --watch --noEmit --incremental --preserveWatchOutput --pretty",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "copynojekyll": "copyfiles .nojekyll ./out/",
    "build": "pnpm run pre-commit && next build && next export && pnpm run copynojekyll",
    "start": "next start",
    "lint": "next lint",
    "depcruise": "depcruise --config .dependency-cruiser.js src",
    "find-deadcode": "ts-prune",
    "prepare": "husky install",
    "pre-commit": "pnpm run ts && pnpm run depcruise && pnpm run find-deadcode && pnpm run lint",
    "deploy": "pnpm run build && node ./scripts/gh-pages.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "axios": "^0.26.1",
    "color.js": "^1.2.0",
    "get-image-colors": "^4.0.1",
    "glob": "^7.2.3",
    "image-type": "^4.1.0",
    "javascript-time-ago": "^2.5.9",
    "jsdom": "^19.0.0",
    "marked-react": "^2.0.0",
    "next": "12.1.1",
    "node-emoji": "^1.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "slate": "^0.102.0",
    "slate-react": "^0.102.0",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/get-image-colors": "^4.0.5",
    "@types/glob": "^7.2.0",
    "@types/javascript-time-ago": "^2.0.8",
    "@types/jsdom": "^16.2.15",
    "@types/node": "17.0.23",
    "@types/node-emoji": "^1.8.2",
    "@types/react": "17.0.43",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "dependency-cruiser": "^11.18.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "husky": "^7.0.4",
    "next-optimized-images": "3.0.0-canary.10",
    "prettier": "^2.8.8",
    "ts-prune": "^0.10.3",
    "typescript": "4.6.3"
  }
}