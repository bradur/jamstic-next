{
  "name": "jamsticnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev-ts": "concurrently -n NEXT,TS -c magenta,cyan \"next dev -p 3001\" \"npm run tsw\"",
    "tsw": "tsc --watch --noEmit --incremental --preserveWatchOutput --pretty",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "copynojekyll": "copyfiles .nojekyll ./out/",
    "build": "npm run pre-commit && next build && next export && npm run copynojekyll",
    "start": "next start",
    "lint": "next lint",
    "depcruise": "depcruise --config .dependency-cruiser.js src",
    "find-deadcode": "ts-prune",
    "prepare": "husky install",
    "pre-commit": "npm run ts && npm run depcruise && npm run find-deadcode && npm run lint",
    "deploy": "npm run build && node ./scripts/gh-pages.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "axios": "^0.26.1",
    "get-image-colors": "^4.0.1",
    "glob": "^7.2.0",
    "image-type": "^4.1.0",
    "javascript-time-ago": "^2.3.13",
    "jsdom": "^19.0.0",
    "marked-react": "^1.0.3",
    "next": "12.1.1",
    "node-emoji": "^1.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "slate": "^0.86.0",
    "slate-react": "^0.86.0",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/get-image-colors": "^4.0.1",
    "@types/glob": "^7.2.0",
    "@types/image-type": "^4.0.1",
    "@types/javascript-time-ago": "^2.0.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "17.0.23",
    "@types/node-emoji": "^1.8.1",
    "@types/react": "17.0.43",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "dependency-cruiser": "^11.4.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^4.0.0",
    "husky": "^7.0.4",
    "next-optimized-images": "^3.0.0-canary.10",
    "prettier": "^2.6.1",
    "ts-prune": "^0.10.3",
    "typescript": "4.6.3"
  }
}
